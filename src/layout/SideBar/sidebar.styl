.layout-menus
  flex 0 0 auto
  width 200px
  height 100%
  background #535353
  overflow hidden
  transition all .3s ease-in-out
  &.collapse
    width 64px
    .el-sub-menu__icon-arrow,
    .menu-title__text
      display none
  &__logo
    width 100%
    height 60px
    text-align center
    line-height 60px
    color #fff
.el-menu
  border-right none !important